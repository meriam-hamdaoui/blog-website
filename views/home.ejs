<%- include("partials/header") -%>
<div class="container-fluid">
  <h1>Home Page</h1>

  <p><%= homeParagraph %></p>

  <% postItems.map(post => { -%>
  <h1><%= post.title %></h1>
  <p>
    <%= post.content.substring(0, 100) + "..." %>
    <a href="/posts/<%=post.title%>">Read More</a>
  </p>
</div>
<% }) %> <%- include("partials/footer") -%>
